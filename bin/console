#!/usr/bin/env php
<?php
declare(strict_types=1);

use Symfony\Component\Console\Application;
use MapMissingItems\Application\Command\MapGapsScanCommand;
use MapMissingItems\Application\Command\ItemsXmlAugmentCommand;
use MapMissingItems\Application\Command\ReportMergeCommand;

require __DIR__ . '/../vendor/autoload.php';

$app = new Application('map-item-gaps', '0.1.0');
$app->add(new MapGapsScanCommand());
$app->add(new ItemsXmlAugmentCommand());
$app->add(new ReportMergeCommand());

$app->run();
